{
    "id": "urn:dev:wot:com:example:servient:lamp",
    "@context": "https://www.w3.org/2019/wot/td/v1",
    "name": "MyLampThing",
    "description": "Invalid TD since op in actions uses camelCase",
    "securityDefinitions": {
        "basic_sc": {
            "scheme": "basic",
            "in": "header"
        }
    },
    "security": ["basic_sc"],
    "actions": {
        "toggle": {
            "forms": [{
                "href": "https://mylamp.example.com/toggle",
                "op": ["invokeAction"]
            }]
        }
    }
}