{
    "id": "urn:required",
    "$comment": "TD that does not respect the status property type",
    "@context": [
        "https://www.w3.org/2022/wot/td/v1.1"
    ],
    "title": "Smart Lamp",
    "description":"Basic usage of TD and TM together",
    "securityDefinitions": {
        "basic_sc": {
            "scheme": "basic",
            "in": "header"
        }
    },
    "security": "basic_sc",
    "properties": {
        "status": {
            "type": "number",
            "description": "current status of the lamp (on|off)",
            "readOnly": false,
            "writeOnly": false,
            "observable": false,
            "enum": [
                "on_value",
                "off_value",
                "error_value"
            ],
            "forms": [
                {
                    "op":"readproperty",
                    "href": "http://127.0.13.232:4563/status"
                }
            ]
        }
    },
    "actions": {},
        "links": [
            {
                "rel": "type",
                "href": "https://raw.githubusercontent.com/thingweb/thingweb-playground/master/packages/core/examples/tms/valid/optional.json",
                "type": "application/td+json"
            }
        ],
        "events": {
            "overheating": {
                "description": "Lamp reaches a critical temperature (overheating)",
                "data": {
                    "type": "string"
                },
                "forms": [
                    {
                        "href": "https://mylamp.example.com/oh",
                        "subprotocol": "longpoll"
                    }
                ]
            }
        }
}